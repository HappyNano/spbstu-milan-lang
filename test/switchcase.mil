begin
    i := 0;
    while i != -1 do
      i := READ;
      switch (i)
      begin
        case 1:
          write (1)
          break;
        case 2:
          write (2)
          break;
        case 3:
          write (3)
          break;
        default:
          write (-1)
      endcase
    od
end
